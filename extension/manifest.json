{
    "name": "Gib",
    "description": "The Gib extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "/images/smile_16.png",
            "32": "/images/smile_32.png",
            "48": "/images/smile_48.png",
            "128": "/images/smile_128.png"
        },
        "default_popup": "gib.html"
    },
    "background": {
        "service_worker": "app.js"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ]
}
